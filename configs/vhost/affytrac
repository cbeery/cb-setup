# Don't cause the server to break if Passenger module not loaded
<IfModule passenger_module>
  <VirtualHost *:80>
    Include /etc/apache2/extra/affytrac_common.conf
  </VirtualHost>

  <VirtualHost *:443>
    Include /etc/apache2/extra/affytrac_common.conf
    SSLEngine on
    SSLCertificateFile /etc/apache2/ssl/star_affytrac_com.crt
    SSLCertificateKeyFile /etc/apache2/ssl/star_affytrac_com.key
    SSLCertificateChainFile /etc/apache2/ssl/DigiCertCA.crt
  </VirtualHost>
</IfModule>
